		// TODO Auto-generated method stub
//
		int number = 12345;
		StringBuffer sb = new StringBuffer();
		do{
			int j = number%10;
		    	    
		    sb.append(j);
		  	number = number/10;
			//System.out.println(number);
			if(number<10)
				sb.append(number);
		}while(number>10);
		System.out.println(sb.reverse());
		
		char ch1[] = {};
		
	}

