package arrayofObjects;

import java.util.ArrayList;
import java.util.List;

public class mainMethod {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		personData person = new personData();
		
		person.setAge(23);
		person.setName("Yogesh");
		
		System.out.println("Person of age "+ person.getAge());
		System.out.println("Person of Name "+ person.getName());
		
		//Array declaration of objects
		personData[] arrayOfPerson= new personData[2];
		
		for(int i = 0;i<2;i++){
			//****Intialise each array member with a class object
			arrayOfPerson[i] = new personData();
			
			arrayOfPerson[i].setAge(34);
			arrayOfPerson[i].setName("Naik");
			//arrayOfPerson[i].age = 34;
		}
		
		System.out.println(" ");
		for(int i = 0;i<=arrayOfPerson.length-1;i++){
			System.out.println("here is the data "+ arrayOfPerson[0].getAge());
			System.out.println("Enter the data for person "+ arrayOfPerson[0].getName());
		}//array of person
			
	}
	
	List<personData[]> p = new ArrayList<personData>();
	

}
