
public class Stack {

	LinkedList li = new LinkedList();

	public void push(int data) {
	    li.insertFirst(data);
	}

	public void pop() {
	    if(!li.isEmpty()){
	    	li.deleteFirst();
	    }
//	    else{
//	    	System.out.println("Stack is empty");
//	    }
	}

	public void displayStack() {
	    System.out.println("  ");
	    while(!li.isEmpty())
	    	li.displayList();
	  }
}
